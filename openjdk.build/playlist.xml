		<testCaseName>ClassLoadingTest</testCaseName>
		<variations>
			<variation></variation>
		</variations>
		<command>cd $(OPENJDK_SYSTEMTEST_ROOT)/openjdk.build && make test.ClassloadingLoadTest JAVA_ARGS=$(Q)$(JVM_OPTIONS)$(Q); \
		$(TEST_STATUS)</command> 
